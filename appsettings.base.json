{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SoapSettings": {
    "Url": "<your-soap-url>",
    "Username": "<your-soap-username>",
    "Password": "<your-soap-password>"
  },
  "ApiAuth": {
    "Clients": [
      { "ApiKey": "<your-api-key>", "ApiSecret": "<your-api-secret>", "Name": "<client-name>" }
    ]
  }
}
